{
    "comments": {
      "lineComment": "#"
    },
    "brackets": [
      ["(", ")"]
    ],
    "autoClosingPairs": [
      {"open": "(", "close": ")"},
      {"open": "\"", "close": "\""}
    ],
    "surroundingPairs": [
      ["(", ")"],
      ["\"", "\""]
    ],
    "indentationRules": {
      "increaseIndentPattern": "^.*ухожу я в пляс.*$",
      "decreaseIndentPattern": "^.*закончили пляски.*$"
    }
  }