{
	"name": "zmey-gorynich-syntax",
	"displayName": "Zmey Gorynich Syntax Highlighting",
	"description": "Syntax highlighting for Zmey Gorynich language (.zg)",
	"version": "0.0.1",
	"engines": {
	  "vscode": "^1.86.0"
	},
	"categories": [
	  "Programming Languages"
	],
	"contributes": {
